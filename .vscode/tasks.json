{
    "tasks": [
        {
            "type": "shell",
            "label": "task g++",
            "command": "g++.exe",
            "args": [
                "-g",
                "D:/github/DS_course_design/lib/*.cpp",
                "D:/github/DS_course_design/src/Problem1.cpp",
                "-o",
                "D:/github/DS_course_design/src/Problem1.exe"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}